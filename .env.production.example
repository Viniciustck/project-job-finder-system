# Exemplo de arquivo .env para produção (Portainer)
# IMPORTANTE: Adicione estes valores como Environment Variables no Portainer Stack

# Database
DB_PASSWORD=sua_senha_postgres_segura_aqui

# Gemini AI
GEMINI_API_KEY=sua_chave_gemini_aqui

# Telegram
TELEGRAM_BOT_TOKEN=seu_bot_token_aqui
TELEGRAM_CHAT_ID=seu_chat_id_aqui

# JSearch API (RapidAPI)
JSEARCH_API_KEY=sua_chave_jsearch_aqui

# Adzuna API
ADZUNA_APP_ID=seu_adzuna_app_id_aqui
ADZUNA_APP_KEY=sua_chave_adzuna_aqui

# Google Search (Opcional - pode deixar vazio)
GOOGLE_SEARCH_KEY=
GOOGLE_SEARCH_CX=
